{
  "parser": "@babel/eslint-parser",
  "extends": ["eslint:recommended"],
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "requireConfigFile": false
  },
  "rules": {
    "semi": [2, "never"],
    "indent": [2, 2],
    "comma-dangle": [2, "never"]
  }
}
